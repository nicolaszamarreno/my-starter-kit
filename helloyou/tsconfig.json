{
    "compilerOptions": {
        "outDir": "./",
        "sourceMap": true,
        "allowJs": false,
        "strict": true,
        "noUnusedLocals": true,
        "inlineSourceMap": false,
        "module": "commonjs",
        "declaration": false,
        "target": "es5",
        "noImplicitAny": true,
        "importHelpers": true,
        "emitDecoratorMetadata": true,
        "jsx": "react",
        "experimentalDecorators": true,
        "lib": ["es2015", "dom"]
    },
    "exclude": ["node_modules"]
}
